{
  "extends": "airbnb",
  "globals": {
    "__DEV__": true
  },
  "env": {
    "es6": true,
    "jest": true
  },
  "rules": {
    "react/jsx-quotes": 0,
    "jsx-quotes": [2, "prefer-double"],
    "func-names": 0,
    "id-length": [2, {"min": 1}],
    "no-param-reassign": 0,
    "eqeqeq": 0,
    "no-unused-expressions": 0,
    "no-console": 0,
    "prefer-const": 0,
    "no-unused-vars": 0,
    "no-shadow": 0,
    "spaced-comment": 0
  }
}
